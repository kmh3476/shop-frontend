{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://shop-backend-1-dfsl.onrender.com/api/$1"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|webp|woff2|woff|ttf|eot|map)$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.$2"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://shop-backend-1-dfsl.onrender.com",
    "VITE_STRAPI_URL": "http://localhost:1337",
    "VITE_STRAPI_TOKEN": "",
    "VITE_BUILDER_PUBLIC_API_KEY": "",
    "VITE_BUILDER_API_URL": "https://cdn.builder.io/api/v3",
    "VITE_BUILDER_MODEL": "page",
    "VITE_BUILDER_PREVIEW": "true",
    "VITE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2|woff|ttf|eot|map)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "redirects": []
}
